#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules/@nestjs/cli/bin/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules/@nestjs/cli/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules/@nestjs/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules/@nestjs/cli/bin/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules/@nestjs/cli/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules/@nestjs/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@nestjs+cli@11.0.10_@swc+cli@0.7.8_@swc+core@1.13.5_chokidar@3.6.0__@swc+core@1.13.5_@types+node@22.18.0/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nest.js" "$@"
else
  exec node  "$basedir/../../bin/nest.js" "$@"
fi
