#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules/@angular-devkit/schematics-cli/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules/@angular-devkit/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules/@angular-devkit/schematics-cli/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules/@angular-devkit/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@angular-devkit+schematics-cli@19.2.15_@types+node@22.18.0_chokidar@4.0.3/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/schematics.js" "$@"
else
  exec node  "$basedir/../../bin/schematics.js" "$@"
fi
