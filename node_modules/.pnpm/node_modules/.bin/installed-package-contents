#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/installed-package-contents/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/installed-package-contents/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@npmcli/installed-package-contents/index.js" "$@"
else
  exec node  "$basedir/../@npmcli/installed-package-contents/index.js" "$@"
fi
