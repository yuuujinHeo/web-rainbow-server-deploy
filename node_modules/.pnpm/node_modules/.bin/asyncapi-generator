#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@asyncapi+generator@1.13.1_@swc+core@1.13.5_@types+babel__core@7.20.5_@types+node@22.18.0_encoding@0.1.13/node_modules/@asyncapi/generator/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@asyncapi+generator@1.13.1_@swc+core@1.13.5_@types+babel__core@7.20.5_@types+node@22.18.0_encoding@0.1.13/node_modules/@asyncapi/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@asyncapi+generator@1.13.1_@swc+core@1.13.5_@types+babel__core@7.20.5_@types+node@22.18.0_encoding@0.1.13/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@asyncapi+generator@1.13.1_@swc+core@1.13.5_@types+babel__core@7.20.5_@types+node@22.18.0_encoding@0.1.13/node_modules/@asyncapi/generator/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@asyncapi+generator@1.13.1_@swc+core@1.13.5_@types+babel__core@7.20.5_@types+node@22.18.0_encoding@0.1.13/node_modules/@asyncapi/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/@asyncapi+generator@1.13.1_@swc+core@1.13.5_@types+babel__core@7.20.5_@types+node@22.18.0_encoding@0.1.13/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@asyncapi/generator/cli.js" "$@"
else
  exec node  "$basedir/../@asyncapi/generator/cli.js" "$@"
fi
