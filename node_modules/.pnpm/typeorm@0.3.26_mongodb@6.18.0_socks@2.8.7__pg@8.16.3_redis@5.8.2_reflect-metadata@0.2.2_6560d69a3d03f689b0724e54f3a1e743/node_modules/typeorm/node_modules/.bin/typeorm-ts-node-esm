#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/typeorm@0.3.26_mongodb@6.18.0_socks@2.8.7__pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_6560d69a3d03f689b0724e54f3a1e743/node_modules/typeorm/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/typeorm@0.3.26_mongodb@6.18.0_socks@2.8.7__pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_6560d69a3d03f689b0724e54f3a1e743/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/typeorm@0.3.26_mongodb@6.18.0_socks@2.8.7__pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_6560d69a3d03f689b0724e54f3a1e743/node_modules/typeorm/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/typeorm@0.3.26_mongodb@6.18.0_socks@2.8.7__pg@8.16.3_redis@5.8.2_reflect-metadata@0.2.2_6560d69a3d03f689b0724e54f3a1e743/node_modules:/home/rainbow/web-rainbow-server-deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
